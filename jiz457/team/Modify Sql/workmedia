INSERT Into test.ns_worksmedium
select
t.itmaddletter,
t.itmaddnumber,
'Israel_Museum',
t.itmaddmedia
from
tblmedia t, tblidenification s
Where
t.itmaddletter = s.idletterkey
And
t.itmaddnumber = s.idnumberkey
And
s.id_museumname = 'Israel_Museum'

;




